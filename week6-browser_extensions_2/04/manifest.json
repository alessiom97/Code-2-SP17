{
	"manifest_version": 2,
	"name": "CSS",
	"version": "1.0",

	"description": "replace page CSS",

	"browser_action": {
		"default_icon": "icon.png"
	},

	"content_scripts": [
		{
			"matches": ["<all_urls>"],
			"js": ["script.js"]
		}
	],

	"permissions": [
		"http://placekitten.com/",
		"http://placehold.it/"
	]

}