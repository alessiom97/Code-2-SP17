{
	"manifest_version": 2,
	"name": "script-generated popup",
	"version": "1.0",

	"description": "opening a page will open a alert",

	"content_scripts": [
		{
			"matches": ["https://www.facebook.com/*"],
			"js": ["script.js"]
		},
		{
			"matches": ["https://twitter.com/*"],
			"js": ["script.js"]
		}
	]

}